{
    "name": "todo_app",
    "version": "1.0.0",
    "description": "todo app for practice",
    "main": "index.js",
    "scripts": {
        "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
        "dev:frontend": "cd app/frontend && npm run dev",
        "dev:backend": "cd app/backend && npm run dev",
        "build": "npm run build:frontend && npm run build:backend",
        "build:frontend": "cd app/frontend && npm run build",
        "build:backend": "cd app/backend && npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git@github-private:nz-programming/todo_app.git"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "concurrently": "^9.2.0"
    }
}
